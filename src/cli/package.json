{
  "name": "ask-interviewers",
  "version": "1.0.0",
  "description": "A CLI tool that provides a curated list of questions to ask during technical interviews",
  "main": "dist/cli/cli.js",
  "bin": {
    "aski": "./dist/cli/cli.js",
    "ask_questions": "./dist/cli/cli.js",
    "ask_interviewers": "./dist/cli/cli.js"
  },
  "type": "module",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "node --experimental-transform-types cli.ts",
    "build": "npm run clean && ./node_modules/.bin/tsc --build tsconfig.json",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "cli",
    "interview",
    "questions",
    "technical-interview",
    "interview-questions",
    "job-interview"
  ],
  "author": "Anubhav Saini <iamanubhavsaini+git@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamanubhavsaini/ask-interviewers.git"
  },
  "bugs": {
    "url": "https://github.com/iamanubhavsaini/ask-interviewers/issues"
  },
  "homepage": "https://github.com/iamanubhavsaini/ask-interviewers#readme",
  "engines": {
    "node": ">=24.0.0"
  },
  "dependencies": {
    "@f0c1s/color-black": "*",
    "@f0c1s/color-blue": "*",
    "@f0c1s/color-cyan": "*",
    "@f0c1s/color-green": "*",
    "@f0c1s/color-magenta": "*",
    "@f0c1s/color-red": "*",
    "@f0c1s/color-white": "*",
    "@f0c1s/color-yellow": "*",
    "commander": "^12.0.0"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/node": "^22.15.17",
    "typescript": "^5.8.3"
  }
}
