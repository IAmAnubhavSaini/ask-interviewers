{
  "name": "ask-interviewers",
  "version": "1.3.0",
  "description": "A CLI tool that provides a curated list of questions to ask during technical interviews",
  "main": "dist/cli.js",
  "bin": {
    "aski": "./dist/cli.js",
    "ask_questions": "./dist/cli.js",
    "ask_interviewers": "./dist/cli.js"
  },
  "type": "module",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "node --experimental-transform-types src/cli.ts",
    "build": "npm run clean && tsc --build tsconfig.json",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "cli",
    "interview",
    "questions",
    "technical-interview",
    "interview-questions",
    "job-interview"
  ],
  "author": "Anubhav Saini <iamanubhavsaini+git@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamanubhavsaini/ask-interviewers.git"
  },
  "bugs": {
    "url": "https://github.com/iamanubhavsaini/ask-interviewers/issues"
  },
  "homepage": "https://github.com/iamanubhavsaini/ask-interviewers#readme",
  "engines": {
    "node": ">=24.0.0"
  },
  "dependencies": {
    "@f0c1s/random-colored-string": "^2026.0.0",
    "commander": "^12.0.0"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/node": "^22.15.17",
    "typescript": "^5.8.3"
  }
}
